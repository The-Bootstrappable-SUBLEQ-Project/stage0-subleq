rem Standard program start
addr ZERO 0
var tmp 0
var tmp2 0
addr SERIAL_OUT 13ED27F0
addr CPU_CONTROL_START 13EE0000
lblsq ZERO ZERO EP

label EP
var a 1
var b 2
jeq a b TEST_FAIL tmp tmp2

set b 0
jeq a b TEST_FAIL tmp tmp2

set a 0
jeq a b TEST_SUCCESS tmp tmp2

label TEST_FAIL
set a 46
putchar a tmp
lbljmp HALT

label TEST_SUCCESS
set a 2e
putchar a tmp

label HALT
set a a
putchar a tmp

rem Stop CPU 0
set_safe CPU_CONTROL_START 2 tmp tmp2

rem Infinite loop
relsq ZERO ZERO 0