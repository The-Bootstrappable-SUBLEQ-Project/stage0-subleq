rem Standard program start
addr ZERO 0
var tmp 0
var tmp2 0
var tmp3 0
addr SERIAL_OUT 13ED27F0
addr CPU_CONTROL_START 13EE0000
lblsq ZERO ZERO EP

label EP
print_qword TEST_DATA tmp tmp2 tmp3

putchar SPACE tmp
incaddr TEST_DATA 8
print_qword TEST_DATA tmp tmp2 tmp3

putchar SPACE tmp
incaddr TEST_DATA 8
print_qword TEST_DATA tmp tmp2 tmp3

putchar SPACE tmp
incaddr TEST_DATA 8
print_qword TEST_DATA tmp tmp2 tmp3

putchar SPACE tmp
incaddr TEST_DATA 8
print_qword TEST_DATA tmp tmp2 tmp3

set tmp a
putchar tmp tmp2

halt tmp tmp2

label TEST_DATA
raw 0000000000000000 ffffffffffffffff 0123456789abcdef fedcba9876543210 f123456789abcdef

label SPACE
rem Notice that there are two spaces
raw_chars  