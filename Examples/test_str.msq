rem Standard program start
addr ZERO 0
var tmp 0
var tmp2 0
var a 0
addr SERIAL_IN 13ED27E8
addr SERIAL_OUT 13ED27F0
addr CPU_CONTROL_START 13EE0000
var FREE_START 1000000
lblsq ZERO ZERO EP

label EP

addr string 10000
alloc_str string 1000 tmp
inp_token string tmp tmp2

strcmp_const string NYAN TEST_FAIL tmp tmp2
strcmp_const string UNG TEST_FAIL tmp tmp2
strcmp_const string GNU TEST_SUCCESS tmp tmp2

label TEST_FAIL
set a 46
putchar a tmp
lbljmp HALT

label TEST_SUCCESS
set a 2e
putchar a tmp

label HALT
set a a
putchar a tmp

halt tmp tmp2

def_string NYAN NYAN
def_string UNG UNG
def_string GNU GNU